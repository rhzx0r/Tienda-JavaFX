<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" style="-fx-background-color: #2D4059;" prefHeight="310" prefWidth="440" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProductRegisterController">
   <children>
      <Label layoutX="110.0" layoutY="15" text="Registrar Producto" textFill="#E5E5E5">
         <font>
            <Font name="Gill Sans MT" size="30" />
         </font>
      </Label>

      <Label layoutX="50" layoutY="84.0" text="Nombre del producto" textFill="#E5E5E5">
         <font>
            <Font name="Gill Sans MT" size="18" />
         </font>
      </Label>

      <Label layoutX="50" layoutY="117.0" text="Precio al Publico" textFill="#E5E5E5">
         <font>
            <Font name="Gill Sans MT" size="18" />
         </font>
      </Label>

      <Label layoutX="50" layoutY="155.0" text="Precio del Proveedor" textFill="#E5E5E5">
         <font>
            <Font name="Gill Sans MT" size="18" />
         </font>
      </Label>

      <Label layoutX="50" layoutY="192.0" text="Cantidad Total" textFill="#E5E5E5">
         <font>
            <Font name="Gill Sans MT" size="18" />
         </font>
      </Label>

      <Button fx:id="btnRegisterProduct" layoutX="80" layoutY="250" mnemonicParsing="false" prefHeight="36.0" prefWidth="120" text="Registrar" textFill="#000000">
         <font>
            <Font name="Gill Sans MT" size="15.0" />
         </font>
      </Button>

      <Button fx:id="btnCancel" layoutX="250" layoutY="250" mnemonicParsing="false" prefHeight="36.0" prefWidth="120" text="Cancelar" textFill="#000000">
         <font>
            <Font name="Gill Sans MT" size="15.0" />
         </font>
      </Button>

      <TextField fx:id="Field_productName" layoutX="220" layoutY="80.0" prefWidth="180" />
      <TextField fx:id="Field_publicPrice" layoutX="220" layoutY="118.0" prefWidth="180" />
      <TextField fx:id="Field_providerPrice" layoutX="220" layoutY="155.0" prefWidth="180" />
      <TextField fx:id="Field_stock" layoutX="220" layoutY="192.0" prefWidth="180" />
      <!-- <Separator layoutX="95.0" layoutY="38.0" orientation="VERTICAL" prefHeight="355.0" prefWidth="7.0" /> -->
   </children>
</AnchorPane>